<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="64.0" prefWidth="1160.0" style="-fx-border-style: hidden hidden solid hidden; -fx-border-color: #eaeaea; -fx-background-color: #212A3E;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dbproject.orderRowController">
   <children>
      <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <children>
            <Text fx:id="oid" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="orderid" />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <children>
            <Text fx:id="cname" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="name" />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <children>
            <Text fx:id="totalPrice" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="totalPrice" />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Text fx:id="totalProducts" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="totalProducts" />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <children>
            <Text fx:id="discount" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="discount" />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Text fx:id="date" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="date" />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Text fx:id="employee" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="employee" />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <children>
            <Button fx:id="moreDetails" mnemonicParsing="false" onAction="#handleMoreDetails" style="-fx-background-color: #394867; -fx-border-radius: 5;" text="More Details" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </FlowPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
